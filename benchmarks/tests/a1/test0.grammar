// grammar that exposes the "false" eval bug in the tool
traversal fuse {
    case Group {
        ??;
        iterate l { ??; recur l; ??; }
        iterate r { ??; recur r; ??; }
        ??;
    }
}
interface Element {
    output id : bool;
}
class Group : Element {
    children {
        l : [Element];
        r : [Element];
    }
    rules {
        self.id := false;
    }
}